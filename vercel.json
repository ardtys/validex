{
  "version": 2,
  "buildCommand": "cd landing-page && npm install && npm run build",
  "outputDirectory": "landing-page/.next",
  "framework": "nextjs",
  "installCommand": "cd landing-page && npm install",
  "devCommand": "cd landing-page && npm run dev",
  "builds": [
    {
      "src": "landing-page/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/landing-page/$1"
    }
  ]
}
